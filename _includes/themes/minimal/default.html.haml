!!! 5
%html
  %head
    %meta{:charset => 'utf-8'}
    %title= '{{ page.title }}'
    :plain
      {% if page.description  %}

    %meta{:name => 'description', :content => '{{ page.description }}'}

    :plain
      {% endif %}

    %script{:src => '{{ ASSET_PATH }}/js'}
    %script{:src => 'https://raw.github.com/processing-js/processing-js/develop/processing.js'}

    /[if lt IE 9]
      %script{:src => 'http://html5shim.googlecode.com/svn/trunk/html5.js'}

    %style{:href => '{{ ASSET_PATH }}/css/1.4.0/bootstrap.css', :rel => 'stylesheet'}
    %style{:href => '{{ ASSET_PATH }}/css/site.css', :rel => 'stylesheet'}

  %body
    .topbar
      .fill
        .container
          %a{:href => '{{ HOME_PATH }}'}
            = '{{ site.title }}'
          %ul.nav
            :plain
              {% assign pages_list = site.pages %}
              {% assign group = 'navigation' %}
              {% include JB/pages_list %}

    %script{:src => 'assets/themes/minimal/js'}
    %script{:src => 'https://raw.github.com/processing-js/processing-js/develop/processing.js'}

    .container
      .content= '{{ content }}'


    %footer
  :javascript
    console.log('hello world!');

  :plain
    {% include JB/analytics %}
